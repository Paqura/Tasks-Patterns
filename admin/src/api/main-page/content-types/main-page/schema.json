{
  "kind": "singleType",
  "collectionName": "main_pages",
  "info": {
    "singularName": "main-page",
    "pluralName": "main-pages",
    "displayName": "MainPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "text",
      "required": true,
      "default": "Technologically preventing the unacceptable for business and government",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subtitle": {
      "type": "text",
      "default": "Bringing experts and enthusiasts together to&nbsp;create cutting-edge cybersecurity technology",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "contactButtonText": {
      "type": "string",
      "required": true,
      "default": "Contact us",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "contactsAnchorText": {
      "type": "string",
      "required": true,
      "default": "Contact",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "headingVideo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "main.advantages-block",
        "main.tools-block",
        "main.products-block",
        "main.analytics-block",
        "main.news-block"
