{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "analytic-article",
    "pluralName": "analytic-articles",
    "displayName": "AnalyticArticles",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "topic": {
      "type": "text",
      "required": true
    },
    "published": {
      "type": "date",
      "required": true
    },
    "tag": {
      "type": "string"
    },
    "articleText": {
      "type": "component",
      "repeatable": true,
      "component": "article-section.article-section"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tableOfContent": {
      "type": "string",
      "default": "Table of contents"
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["files", "images", "videos", "audios", "files"]
    },
    "titleOfHelpfulFiles": {
      "type": "string"
    }
  }
}
