{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "analytic-article",
    "pluralName": "analytic-articles",
    "displayName": "AnalyticArticles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "topic": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "published": {
      "type": "date",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tag": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "articleText": {
      "type": "component",
      "repeatable": true,
      "component": "article-section.article-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tableOfContent": {
      "type": "string",
      "default": "Table of contents",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files",
        "images",
        "videos",
        "audios",
        "files"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "titleOfHelpfulFiles": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
